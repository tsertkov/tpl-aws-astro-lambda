{
    "name": "@tpl-aws-astro-lambda/api",
    "type": "module",
    "version": "0.0.1",
    "scripts": {
        "prettier:check": "prettier --check .",
        "eslint:check": "eslint .",
        "prettier:fix": "prettier --write .",
        "eslint:fix": "eslint --fix .",
        "test:unit": "c8 --check-coverage --lines 100 --all -x eslint.config.js -x src/test/ node --test-reporter spec --test",
        "test": "npm run prettier:check && npm run eslint:check && npm run test:unit",
        "format": "npm run prettier:fix && npm run eslint:fix"
    },
    "engines": {
        "node": ">=20"
    },
    "devDependencies": {
        "@types/aws-lambda": "^8.10.138",
        "aws-sdk-client-mock": "^4.0.0",
        "c8": "^9.1.0",
        "esbuild": "^0.21.3",
        "eslint": "^9.3.0",
        "prettier": "^3.2.5"
    }
}
